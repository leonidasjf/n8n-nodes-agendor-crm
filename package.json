{
  "name": "n8n-nodes-agendor-crm",
  "version": "1.0.1",
  "description": "Node oficial do n8n para integração completa com Agendor CRM",
  "keywords": [
    "n8n-community-node-package",
    "agendor",
    "crm",
    "sales",
    "integration"
  ],
  "license": "MIT",
  "homepage": "https://github.com/leonidasjf/n8n-nodes-agendor-crm",
  "author": "Leônidas Caldeira",
  "repository": {
    "type": "git",
    "url": "https://github.com/leonidasjf/n8n-nodes-agendor-crm.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=8.1"
  },
  "packageManager": "pnpm@8.1.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npx gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "echo 'Lint skipped'",
    "lintfix": "echo 'Lint fix skipped'",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "assets"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AgendorApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Agendor/Agendor.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}